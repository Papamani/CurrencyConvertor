pipeline {
libraries{
lib 'shlib'
}
agent any

    
    tools{
    maven 'Maven'
    }
           
 
    stages {
      stage('Start')
            {
                steps
                 { 
                    sendNotifications 'STARTED'
                    
                 }
            }
 
      stage('clean')
            {
                steps
                 { 
                 sendNotifications 'STARTEDBUILD'
                build 'BUILD'
                 }
            }
}
post{
always{
sendNotifications currentBuild.result
}
}
}
