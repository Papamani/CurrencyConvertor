pipeline {
libraries{
lib 'lib'
}
agent any

    
    tools{
    maven 'Maven'
    }
           
 
    stages {
      stage('Start')
            {
                steps
                 { 
                    sendNotifications 'STARTED'
                    email  Run.mail('revathisaraiswathi@gmail.com')
                    
                 }
            }
 
      stage('clean')
            {
                steps
                 { 
                 sendNotifications 'STARTEDBUILD'
                build 'BUILD'
                 }
            }
      stage('Startnexus')
            {
                steps
                 { 
                    sonar 'SONARSTARTED'
                 }
            }
             stage('Startsonar')
            {
                steps
                 { 
                    sonar 'Sonarstagestarted'
                 }
            }
}
post{
always{
sendNotifications currentBuild.result
}
}
}
