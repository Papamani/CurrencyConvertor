pipeline {
agent any
libraries{
lib 'shlib'
lib 'shlib@sonar.groovy'
lib 'shlib@pipline.groovy'
lib 'shlib@devenvironment.groovy'
lib 'shlib@testingenv.groovy'
lib 'shlib@deployperformanceenv.groovy'
lib 'shlib@deployproduenv.groovy'
}
    
    tools{
    maven 'Maven'
    }
pipeline(branch: 'master', scmUrl='https://github.com/REVATHISARAISWATHI/CurrencyConvertor.git',
                   email: 'revathisaraiswathi@gmail.com', serverPort: '8080',
                  )
           
 
    stages {
      stage('Start')
            {
                steps
                 { 
                    sendNotifications 'STARTED'
                 }
            }
        stage('checkout git') {
            steps {
                git branch: branch, credentialsId: 'GitCredentials', url: scmUrl
            }
        }
           
      stage('clean')
            {
                steps
                 { 
                 sendNotifications 'STARTEDBUILD'
                 sh 'mvn clean install'
                 }
            }
      stage('Startnexus')
            {
                steps
                 { 
                    sonar 'STARTEDNEX'
                 }
            }
             stage('Startsonar')
            {
                steps
                 { 
                    sonar 'Sonarstagestarted'
                 }
            }
}
post{
always{
sendNotifications currentBuild.result
}
}
}
