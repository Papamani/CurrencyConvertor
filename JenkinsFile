pipeline {
    agent any
 
    stages {

      stage('clean')
            {
                steps
                 { 
                    sh 'mvn clean install'
                 }
            }
    stage("deploy")
    {
        steps{
            deploy adapters: [tomcat9(credentialsId: 'tocat',  url: 'http://3.15.0.139:8088')], contextPath: 'deploy', onFailure: false, war: '**/*.war'        
            }
    }
}
}
