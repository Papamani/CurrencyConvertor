pipeline {
libraries{
lib 'shlib'
lib 'shlib@sonar.groovy'
}
    agent any
    tools{
    maven 'Maven'
    }
 
    stages {
      stage('Start')
            {
                steps
                 { 
                    sendNotifications 'STARTED'
                 }
            }
  
      stage('clean')
            {
                steps
                 { 
                    sh 'mvn clean install'
                 }
            }
      stage('Startnexus')
            {
                steps
                 { 
                    sendNotifications 'STARTEDNEX'
                 }
            }
             stage('Startsonar')
            {
                steps
                 { 
                    sendNotifications 'Sonarstagestarted'
                 }
            }
}
post{
always{
sendNotifications currentBuild.result
}
}
}
