pipeline {
agent any
libraries{
lib 'shlib'
lib 'shlib@sonar.groovy'
lib 'shlib@pipline.groovy'
lib 'shlib@devenvironment.groovy'
lib 'shlib@testingenv.groovy'
lib 'shlib@deployperformanceenv.groovy'
lib 'shlib@deployproduenv.groovy'
lib 'shlib@build.groovy'
}
    
    tools{
    maven 'Maven'
    }
           
 
    stages {
      stage('Start')
            {
                steps
                 { 
                    sendNotifications 'STARTED'
                 }
            }
 
      stage('clean')
            {
                steps
                 { 
                 sendNotifications 'STARTEDBUILD'
                build 'BUILD'
                 }
            }
      stage('Startnexus')
            {
                steps
                 { 
                    sonar 'SONARSTARTED'
                 }
            }
             stage('Startsonar')
            {
                steps
                 { 
                    sonar 'Sonarstagestarted'
                 }
            }
}
post{
always{
sendNotifications currentBuild.result
}
}
}
