pipeline {
agent any
environment
{
 myDeliveryPipeline(branch: 'master', scmUrl: 'https://github.com/REVATHISARAISWATHI/CurrencyConvertor.git',
                   email: 'revathisariswathi@gmail.com', serverPort: '8080',
                   )
                   
                   }
  
libraries{
lib 'shlib'
lib 'shlib@sonar.groovy'
}
    
    tools{
    maven 'Maven'
    }
 
    stages {
      stage('Start')
            {
                steps
                 { 
                    sendNotifications 'STARTED'
                 }
            }
           
      stage('clean')
            {
                steps
                 { 
                 sendNotifications 'STARTEDBUILD'
                 sh 'mvn clean install'
                 }
            }
      stage('Startnexus')
            {
                steps
                 { 
                    sonar 'STARTEDNEX'
                 }
            }
             stage('Startsonar')
            {
                steps
                 { 
                    sonar 'Sonarstagestarted'
                 }
            }
}
post{
always{
sendNotifications currentBuild.result
}
}
}
